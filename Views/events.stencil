{% extends "layouts/default.stencil" %}

{% block title %}Notes{% endblock %}

{% block content %}
  There are {{ events.count }} events. <br>
  
  {% for event in events %}
    <div>
      <h2 class="mono">{{ event.title }}</h2>
      {{ event.date }}<br>
      <p>{{ event.description }}</p>
    </div>
  {% endfor %}
{% endblock %}
